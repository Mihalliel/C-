using System;

namespace ConsoleCreatures
{
  class Program
  {
    static void Main()
    {
      // Creating some ASCII art because why not?
      Console.WriteLine(" .-.");    
      Console.WriteLine("(o o)");
      Console.WriteLine("| 0 |");
      Console.WriteLine("|   |");
      Console.WriteLine("'~~~'");
      Console.WriteLine("Spooky Weirdo will get me in a new career path!");
     }
  }
}
